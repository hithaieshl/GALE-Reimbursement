    "lint:js": "eslint --ext .js --ext .jsx client",
    "lint:scss": "stylelint 'client/**/*.scss' --syntax scss",
    "lint": "npm run lint:js && npm run lint:scss",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "cpx 'public/**/*' dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build:dev": "npm run prebuild && npm run env:dev -- webpack",
    "build:qa": "npm run prebuild && npm run env:qa -- webpack",
    "build:uat": "npm run prebuild && npm run env:uat -- webpack",
    "build": "npm run env:prod -- webpack"
